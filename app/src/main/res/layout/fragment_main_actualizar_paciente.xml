<?xml version="1.0" encoding="utf-8"?>
<android.widget.LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Medicos.Medicos">


    <TextView
        android:id="@+id/emcabezado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:contentDescription="@string/title_activity_enfermeras"
        android:text="Seleccione Un Paciente Para Acutallizar su Expediente"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2" />

    <ListView
        android:id="@+id/lista_expedientes"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</android.widget.LinearLayout>