        </activity>
        <activity
            android:name=".Enfermeras.Enfermeras"
            android:label="@string/title_activity_enfermeras"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Medicos.Medicos"
            android:label="@string/title_activity_medicos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Pacientes.Paciente"
            android:label="@string/title_activity_paciente"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Enfermeras.Expediente" />
        <activity android:name=".Enfermeras.Aprobar"></activity>
    </application>

</manifest>